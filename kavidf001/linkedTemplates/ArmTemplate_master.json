{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kavidf001"
		},
		"DS_Output_blobtosql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DS_Output_blobtosql'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=kavisqldbserver001.database.windows.net;Initial Catalog=kavidb001"
		},
		"LS_KaviBlobConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_KaviBlobConnection'"
		},
		"LS_KaviBlobConnection002_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_KaviBlobConnection002'"
		},
		"LS_KaviDatalakegen2Connection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_KaviDatalakegen2Connection'"
		},
		"LS_AzureKeyVault001_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kavikeyvault001.vault.azure.net/"
		},
		"LS_HttpServer001_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().BaseURl}"
		},
		"LS_KaviDatalakegen2Connection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://kavidlgen2001.dfs.core.windows.net/"
		},
		"LS_Kavi_Datalakegen2002_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://kavidlgen2001.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "kavidf001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DS_Output_blobtosql_connectionString": {
						"value": "[parameters('DS_Output_blobtosql_connectionString')]"
					},
					"LS_KaviBlobConnection_connectionString": {
						"value": "[parameters('LS_KaviBlobConnection_connectionString')]"
					},
					"LS_KaviBlobConnection002_connectionString": {
						"value": "[parameters('LS_KaviBlobConnection002_connectionString')]"
					},
					"LS_KaviDatalakegen2Connection_accountKey": {
						"value": "[parameters('LS_KaviDatalakegen2Connection_accountKey')]"
					},
					"LS_AzureKeyVault001_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AzureKeyVault001_properties_typeProperties_baseUrl')]"
					},
					"LS_HttpServer001_properties_typeProperties_url": {
						"value": "[parameters('LS_HttpServer001_properties_typeProperties_url')]"
					},
					"LS_KaviDatalakegen2Connection_properties_typeProperties_url": {
						"value": "[parameters('LS_KaviDatalakegen2Connection_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "kavidf001_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_Kavi_Datalakegen2002_properties_typeProperties_url": {
						"value": "[parameters('LS_Kavi_Datalakegen2002_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'kavidf001_ArmTemplate_0')]"
			]
		},
		{
			"name": "kavidf001_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'kavidf001_ArmTemplate_1')]"
			]
		}
	]
}